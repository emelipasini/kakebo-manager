{
    "name": "kakebo-manager",
    "version": "1.0.0",
    "description": "A project for expenses tracking",
    "main": "dist/index.js",
    "type": "module",
    "scripts": {
        "build": "rm -rf ./dist && tsc",
        "start": "node ./dist/index.js",
        "check-types": "npx tsc --noEmit",
        "lint": "npx eslint ./src/**/*.{ts}",
        "prepare": "husky install"
    },
    "engines": {
        "npm": ">=9.5.1",
        "node": ">=18.16.0"
    },
    "keywords": [
        "typescript",
        "node",
        "chalk",
        "inquirer",
        "eslint",
        "husky",
        "commitlint"
    ],
    "author": "emelipasini",
    "license": "MIT",
    "dependencies": {
        "chalk": "5.3.0",
        "config": "3.3.9",
        "inquirer": "9.2.8"
    },
    "devDependencies": {
        "@commitlint/cli": "17.6.6",
        "@commitlint/config-conventional": "17.6.6",
        "@jest/globals": "29.6.1",
        "@types/config": "3.3.0",
        "@types/inquirer": "9.0.3",
        "@types/jest": "29.5.3",
        "@types/node": "20.4.1",
        "@typescript-eslint/eslint-plugin": "5.62.0",
        "eslint": "8.44.0",
        "eslint-config-standard-with-typescript": "36.0.0",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-n": "15.7.0",
        "eslint-plugin-promise": "6.1.1",
        "husky": "8.0.0",
        "jest": "29.6.1",
        "lint-staged": "13.2.3",
        "ts-jest": "29.1.1",
        "typescript": "5.1.6"
    }
}
